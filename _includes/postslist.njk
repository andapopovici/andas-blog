{% for post in postslist | reverse %}
  <article class="post-entry">
    <header>
    <h3 class="post-title"><a href="{{ post.url | url }}">{% if post.data.title %}{{ post.data.title }}{% else %}<code>{{ post.url }}</code>{% endif %}</a></h3>
    <time datetime="{{ post.date | htmlDateString }}">{{ post.date | readableDate }}</time>
    </header>
  </article>
{% endfor %}
